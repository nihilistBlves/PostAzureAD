@inject UserManager<IdentityUser> UserManager;@inject SignInManager<IdentityUser> SignInManager;@if (SignInManager.IsSignedIn(User)) {    <form method="post" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home")">        <span style="color:dimgrey">            @UserManager.GetUserName(User)        </span>        <button type="submit" class="btn btn-outline-danger">Logout</button>    </form>}